{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "always"
    ],
    "quotemark": [
      true,
      "double",
      "avoid-escape"
    ],
    "no-return-await": true,
    "trailing-comma": [
      true,
      {
        "multiline": "never",
        "singleline": "never"
      }
    ],
    "no-var-requires": false,
    "curly": false,
    "no-trailing-whitespace": true,
    "whitespace": [
      true,
      "check-separator",
      "check-operator",
      "check-module"
    ],
    "space-before-function-paren": false,
    "max-line-length": [
      true,
      120
    ],
    "no-empty": false
  },
  "rulesDirectory": []
}